<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="590.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController2">
   <children>
   
      <ComboBox fx:id="comboBox" layoutX="150.0" layoutY="24.0" onAction="#ComboChanged" prefHeight="26.0" prefWidth="129.0" promptText="Select Name" /> 
  
       <Label fx:id="lblDisplay" alignment="CENTER" layoutX="12.0" layoutY="14.0" prefHeight="46.0" prefWidth="123.0" text="Display:" />
       
      <Button layoutX="14.0" layoutY="67.0" mnemonicParsing="false" onAction="#buttonAction" text="ADD" />
      
      <ListView fx:id="listview" layoutX="92.0" layoutY="60.0" prefHeight="200.0" prefWidth="200.0" />
     
      <Label fx:id="lab1" layoutX="20.0" layoutY="109.0" text="Label" />
      <TreeView fx:id="treeView" layoutX="341.0" layoutY="60.0" onContextMenuRequested="#mouseClick" onMouseClicked="#mouseClick" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="btn1" layoutX="34.0" layoutY="502.0" mnemonicParsing="false" onAction="#buttonAction1" text="Select  One File" />
      <Button fx:id="btn2" layoutX="420.0" layoutY="502.0" mnemonicParsing="false" onAction="#buttonAction2" text="Select Multiple Files" />
      <ListView fx:id="listview2" layoutX="20.0" layoutY="295.0" prefHeight="200.0" prefWidth="542.0" />
   </children>
</AnchorPane>
